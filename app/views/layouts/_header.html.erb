<header class="navbar navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <%= link_to image_tag("dreamhomelogo.png" ,size:"180x100"), root_path, id: "logo" %>
        <nav>
          <ul class= "nav pull-right">
	          <%= render 'layouts/basic_tabs'%>
	        <% if signed_in? && current_user.admin? %>
	          <%= render 'layouts/admin' %>
	        <% elsif signed_in? && current_user.employee? %>
	          <%= render 'layouts/employee'%>
	        <% elsif signed_in? %>
	          <%= render 'layouts/normal_users'%>
	        <% else %>
              <li><%= link_to "Sign in" , signin_path , title: "Sign in"%></li>
            <% end %>
        </ul>
       </nav>
     </div>
     </div>
    </header>